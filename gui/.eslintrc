{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": ["error", 4],
        "curly": ["error", "all"],
        "no-trailing-spaces": ["error"],
        "no-multiple-empty-lines": ["error"],
        "no-unreachable": ["error"],
        "valid-typeof": ["error"],
        "eqeqeq": ["error"]
    }
}