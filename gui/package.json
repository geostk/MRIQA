{
  "name": "mriqa",
  "version": "0.0.0",
  "description": "GUI for MRIQA",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "npm run watch-js & npm run start",
    "watch-js": "watchify public/js/main.js -t [ babelify --presets [ es2015 ] ] -o public/js/bundle.js",
    "test-serverside": "eslint *.js",
    "test-clientside": "eslint public/js/*.js --quiet && eslint public/js/**/*.js && eslint public/js/**/**/*.js",
    "test": "npm run test-serverside && npm run test-clientside"
  },
  "author": "Christian Decker",
  "license": "Apache-2.0",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "express": "^4.15.3",
    "jquery": "^3.2.1",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "eslint": "^3.19.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "watchify": "^3.9.0"
  }
}
